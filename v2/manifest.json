{
    "version": "1.0.0",
    "name": "Not Enough",
    "description": "A Chrome extension to enhance your browsing experience.",
    "manifest_version": 3,
    "icons": {
        "32": "fire.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}